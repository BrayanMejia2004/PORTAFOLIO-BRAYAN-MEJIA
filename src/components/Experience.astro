---
import ExperienceItems from "../components/ExperienceItems.astro";

const EXPERIENCE = [
  {
    date: "Enero 2020 - Actualmente",
    title: "Tecnólogo En Desarrollo De Sistemas Informáticos",
    description:
      "Estudiante de la Universidad Unidades Tecnólogicas de santander, en donde me dio la oportunidad de adquirir grandes conocimientos sobre la programación, y el gusto por el Diseño Web.",
  }
];
---

<ol class="relative my-10">
  {
    EXPERIENCE.map((experience) => (
      <li class="mb-5 ms-4">
        <ExperienceItems {... experience}/>
      </li>
    ))
  }
</ol>
